@using StandardEng.Web.Reports

@{
    var typeReportSource = new TypeReportSource { TypeName = typeof(AMCQuotation).AssemblyQualifiedName };
    typeReportSource.Parameters.Add("AMCQuotationId", ViewBag.AMCQuotationId);
    typeReportSource.Parameters.Add("Reportname", ViewBag.Reportname);
}
<style>
    #RpAMCQuotation {
        position: absolute;
        left: 5px;
        right: 5px;
        top: 5px;
        bottom: 5px;
        overflow: hidden;
    }
</style>

@(Html.TelerikReporting().ReportViewer()
           .Id("RpAMCQuotation")
           .ServiceUrl(WebHelper.ReportApiController)
           .TemplateUrl(WebHelper.ReportTemplate)
           .ReportSource(typeReportSource)
           .ViewMode(ViewMode.PrintPreview)
           .ScaleMode(ScaleMode.Specific)
           .Scale(1.0)
           .PersistSession(false)
)

